    <item>
      <title>Lounge chairs</title>
      <link>http://localhost:1313/docs/roblox/furniture/lounge_chairs/</link>
      <pubDate>Tue, 25 Jun 2024 16:21:19 -0700</pubDate>
      
      <guid>http://localhost:1313/docs/roblox/furniture/lounge_chairs/</guid>
      <description>
        
        
